<p style="color: green"><%= notice %></p>

<h1>Equipable stats</h1>

<div id="equipable_stats">
  <% @equipable_stats.each do |equipable_stat| %>
    <%= render equipable_stat %>
    <p>
      <%= link_to "Show this equipable stat", equipable_stat %>
    </p>
  <% end %>
</div>

<%= link_to "New equipable stat", new_equipable_stat_path %>
