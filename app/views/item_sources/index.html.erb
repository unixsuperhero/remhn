<p style="color: green"><%= notice %></p>

<h1>Item sources</h1>

<div id="item_sources">
  <% @item_sources.each do |item_source| %>
    <%= render item_source %>
    <p>
      <%= link_to "Show this item source", item_source %>
    </p>
  <% end %>
</div>

<%= link_to "New item source", new_item_source_path %>
