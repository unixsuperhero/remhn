<p style="color: green"><%= notice %></p>

<h1>Monster elements</h1>

<div id="monster_elements">
  <% @monster_elements.each do |monster_element| %>
    <%= render monster_element %>
    <p>
      <%= link_to "Show this monster element", monster_element %>
    </p>
  <% end %>
</div>

<%= link_to "New monster element", new_monster_element_path %>
